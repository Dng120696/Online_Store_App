Home Page
<div>
  <%= link_to 'Dashboard', customer_client_dashboard_index_path %>
</div>
<div class="flex items-center gap-4">
  <% if !user_signed_in? && !admin_signed_in? %>
    <%= link_to 'User Sign In', new_user_session_path, class: ' ' %>
    <%= link_to 'Admin Sign In', new_admin_session_path, class: ' ' %>
  <% else %>
    <% if current_user %>
      <%= button_to "Logout", destroy_user_session_path, method: :delete, data: {
                confirm: "Are you sure?"
              } %>
    <% else %>
      <%= button_to "Logout", destroy_admin_session_path, method: :delete, data: {
                confirm: "Are you sure?"
              } %>
    <% end %>
  <% end %>
</div>