<aside id="logo-sidebar" class="top-0 left-0 z-40 h-screen pt-20 transition-transform -translate-x-full bg-white border-gray-200 sm:translate-x-0 flex flex-col min-h-screen border-r-[1px] border-r-gray-300 text-lg" aria-label="Sidebar">
  <div class="h-full px-3 pb-4 overflow-y-auto bg-white flex flex-col">
    <div class="flex items-center mb-7">
      <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6"  viewBox="0 0 576 512">
        <path fill="#3056d3" d="M547.6 103.8L490.3 13.1C485.2 5 476.1 0 466.4 0H109.6C99.9 0 90.8 5 85.7 13.1L28.3 103.8c-29.6 46.8-3.4 111.9 51.9 119.4c4 .5 8.1 .8 12.1 .8c26.1 0 49.3-11.4 65.2-29c15.9 17.6 39.1 29 65.2 29c26.1 0 49.3-11.4 65.2-29c15.9 17.6 39.1 29 65.2 29c26.2 0 49.3-11.4 65.2-29c16 17.6 39.1 29 65.2 29c4.1 0 8.1-.3 12.1-.8c55.5-7.4 81.8-72.5 52.1-119.4zM499.7 254.9l-.1 0c-5.3 .7-10.7 1.1-16.2 1.1c-12.4 0-24.3-1.9-35.4-5.3V384H128V250.6c-11.2 3.5-23.2 5.4-35.6 5.4c-5.5 0-11-.4-16.3-1.1l-.1 0c-4.1-.6-8.1-1.3-12-2.3V384v64c0 35.3 28.7 64 64 64H448c35.3 0 64-28.7 64-64V384 252.6c-4 1-8 1.8-12.3 2.3z"/>
      </svg>
      <span class="ml-2 font-semibold text-blue-600">Online Store</span>
    </div>
    <ul class="space-y-2 font-medium flex-grow">
      <%= link_to owner_dashboard_index_path, class: "py-3 font-semibold flex items-center p-2 text-gray-900 rounded-lg hover:bg-blue-400 hover:text-white group #{request.path == owner_dashboard_index_path  ? 'bg-blue-600 text-white' : ''}" do %>
        <svg class="w-5 h-5 white transition duration-75 group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 22 21">
          <path d="M16.975 11H10V4.025a1 1 0 0 0-1.066-.998 8.5 8.5 0 1 0 9.039 9.039.999.999 0 0 0-1-1.066h.002Z"/>
          <path d="M12.5 0c-.157 0-.311.01-.565.027A1 1 0 0 0 11 1.02V10h8.975a1 1 0 0 0 1-.935c.013-.188.028-.374.028-.565A8.51 8.51 0 0 0 12.5 0Z"/>
        </svg>
        <span class="ms-3">Dashboard</span>
      <% end %>
      <%= link_to owner_products_path, class: "py-3 font-semibold flex items-center p-2 text-gray-900 rounded-lg hover:bg-blue-400 hover:text-white group #{request.path == owner_products_path  ? 'bg-blue-600 text-white' : ''}" do %>
        <svg class="w-5 h-5 white transition duration-75 group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 22 21">
          <path d="M17 5.923A1 1 0 0 0 16 5h-3V4a4 4 0 1 0-8 0v1H2a1 1 0 0 0-1 .923L.086 17.846A2 2 0 0 0 2.08 20h13.84a2 2 0 0 0 1.994-2.153L17 5.923ZM7 9a1 1 0 0 1-2 0V7h2v2Zm0-5a2 2 0 1 1 4 0v1H7V4Zm6 5a1 1 0 1 1-2 0V7h2v2Z"/>
        </svg>
        <span class="ms-3">Products</span>
      <% end %>
      <%= link_to owner_customers_path, class: "py-3 font-semibold flex items-center p-2 text-gray-900 rounded-lg hover:bg-blue-400 hover:text-white group #{request.path == owner_customers_path  ? 'bg-blue-600 text-white' : ''}" do %>
        <svg class="w-5 h-5 white transition duration-75 group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 22 21">
          <path d="M14 2a3.963 3.963 0 0 0-1.4.267 6.439 6.439 0 0 1-1.331 6.638A4 4 0 1 0 14 2Zm1 9h-1.264A6.957 6.957 0 0 1 15 15v2a2.97 2.97 0 0 1-.184 1H19a1 1 0 0 0 1-1v-1a5.006 5.006 0 0 0-5-5ZM6.5 9a4.5 4.5 0 1 0 0-9 4.5 4.5 0 0 0 0 9ZM8 10H5a5.006 5.006 0 0 0-5 5v2a1 1 0 0 0 1 1h11a1 1 0 0 0 1-1v-2a5.006 5.006 0 0 0-5-5Z"/>
        </svg>
        <span class="ms-3">Customers</span>
      <% end %>
      <%= link_to owner_orders_path, class: "py-3 font-semibold flex items-center p-2 text-gray-900 rounded-lg hover:bg-blue-400 hover:text-white group #{request.path == owner_orders_path  ? 'bg-blue-600 text-white' : ''}" do %>
        <svg class="w-5 h-5 white transition duration-75 group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 22 21">
          <path d="M5 5V.13a2.96 2.96 0 0 0-1.293.749L.879 3.707A2.96 2.96 0 0 0 .13 5H5Z"/>
          <path d="M6.737 11.061a2.961 2.961 0 0 1 .81-1.515l6.117-6.116A4.839 4.839 0 0 1 16 2.141V2a1.97 1.97 0 0 0-1.933-2H7v5a2 2 0 0 1-2 2H0v11a1.969 1.969 0 0 0 1.933 2h12.134A1.97 1.97 0 0 0 16 18v-3.093l-1.546 1.546c-.413.413-.94.695-1.513.81l-3.4.679a2.947 2.947 0 0 1-1.85-.227 2.96 2.96 0 0 1-1.635-3.257l.681-3.397Z"/>
          <path d="M8.961 16a.93.93 0 0 0 .189-.019l3.4-.679a.961.961 0 0 0 .49-.263l6.118-6.117a2.884 2.884 0 0 0-4.079-4.078l-6.117 6.117a.96.96 0 0 0-.263.491l-.679 3.4A.961.961 0 0 0 8.961 16Zm7.477-9.8a.958.958 0 0 1 .68-.281.961.961 0 0 1 .682 1.644l-.315.315-1.36-1.36.313-.318Zm-5.911 5.911 4.236-4.236 1.359 1.359-4.236 4.237-1.7.339.341-1.699Z"/>
        </svg>
        <span class="ms-3">Orders</span>
      <% end %>
      <%= link_to owner_categories_path, class: "py-3 font-semibold flex items-center p-2 text-gray-900 rounded-lg hover:bg-blue-400 hover:text-white group #{request.path == owner_categories_path  ? 'bg-blue-600 text-white' : ''}" do %>
        <svg class="w-5 h-5 white transition duration-75 group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 22 21">
          <path d="M6.143 0H1.857A1.857 1.857 0 0 0 0 1.857v4.286C0 7.169.831 8 1.857 8h4.286A1.857 1.857 0 0 0 8 6.143V1.857A1.857 1.857 0 0 0 6.143 0Zm10 0h-4.286A1.857 1.857 0 0 0 10 1.857v4.286C10 7.169 10.831 8 11.857 8h4.286A1.857 1.857 0 0 0 18 6.143V1.857A1.857 1.857 0 0 0 16.143 0Zm-10 10H1.857A1.857 1.857 0 0 0 0 11.857v4.286C0 17.169.831 18 1.857 18h4.286A1.857 1.857 0 0 0 8 16.143v-4.286A1.857 1.857 0 0 0 6.143 10Zm10 0h-4.286A1.857 1.857 0 0 0 10 11.857v4.286c0 1.026.831 1.857 1.857 1.857h4.286A1.857 1.857 0 0 0 18 16.143v-4.286A1.857 1.857 0 0 0 16.143 10Z"/>
        </svg>
        <span class="ms-3">Categories</span>
      <% end %>
      <%= link_to owner_chatbots_path, class: "py-3 font-semibold flex items-center p-2 text-gray-900 rounded-lg hover:bg-blue-400 hover:text-white group #{request.path == owner_chatbots_path  ? 'bg-blue-600 text-white' : ''}" do %>
        <svg class="w-5 h-5 white transition duration-75 group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 24 24">
          <path d="m17.418 3.623-.018-.008a6.713 6.713 0 0 0-2.4-.569V2h1a1 1 0 1 0 0-2h-2a1 1 0 0 0-1 1v2H9.89A6.977 6.977 0 0 1 12 8v5h-2V8A5 5 0 1 0 0 8v6a1 1 0 0 0 1 1h8v4a1 1 0 0 0 1 1h2a1 1 0 0 0 1-1v-4h6a1 1 0 0 0 1-1V8a5 5 0 0 0-2.582-4.377ZM6 12H4a1 1 0 0 1 0-2h2a1 1 0 0 1 0 2Z"/>
        </svg>
        <span class="ms-3">Messages</span>
      <% end %>
    </ul>
    <div class="mt-auto">
      <%= link_to destroy_admin_session_path, data: {
                           turbo_method: :delete,
                           turbo_confirm: "Are you sure?"
                        }, class: "py-3 font-semibold flex items-center p-2 text-gray-900 rounded-lg hover:bg-blue-400 hover:text-white" do %>
        <svg class="w-5 h-5 mr-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 24 24">
          <path d="M17 13v-2H9V9.586L11.707 6.879 10.293 5.465 6 9.758l4.293 4.293 1.414-1.414L9 10.414V11h8zM4 3v18c0 1.103.897 2 2 2h12c1.103 0 2-.897 2-2V3c0-1.103-.897-2-2-2H6c-1.103 0-2 .897-2 2zm16 18H6V3h12v18z"/>
        </svg>
        <span class="ms-3">Logout</span>
      <% end %>
    </div>
  </div>
</aside>
